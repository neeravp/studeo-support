{
    "name": "studeo/support",
    "description": "Utilities and Helpers for Laravel",
    "keywords": [
        "Laravel",
        "Utilities",
        "Helpers",
        "Studeo"
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0",
        "illuminate/filesystem": "^8.0",
        "illuminate/console": "^8.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.4",
        "pestphp/pest": "^1.12",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.3",
        "orchestra/testbench": "^6.15"
    },
    "autoload": {
        "psr-4": {
            "Studeo\\Support\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Studeo\\Support\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Studeo\\Support\\SupportServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Neerav Pandya",
            "email": "me@neerav.dev",
            "role": "Developer"
        }
    ]
}